<h1>sign in</h1>

<form id="login-form" action="/users/create-session" method="POST">
  <input type="email" name="email" placeholder="Your Email" required />
  <input type="password" name="password" placeholder="password" required />
  <button
    class="g-recaptcha"
    data-sitekey="6LcMT1AjAAAAAGt3lBAghqdTkbxFvRVT0FyNOQYZ"
    data-callback="onLoginFormSubmit"
  >
    Sign In
  </button>
  <br />
</form>

<p>
  <a href="/users/auth/google"> Google Sign In/Sign Up</a>
</p>

<form action="/users/create-reset-password-token" method="post">
  <input type="email" name="email" placeholder="email" required />
  <input type="submit" value="Forgot Password" />
</form>
